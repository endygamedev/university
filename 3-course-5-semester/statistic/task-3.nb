Cell[BoxData[{
 RowBox[{"Clear", "[", "empiricalDistributionDensity", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"empiricalDistributionDensity", "[", 
   RowBox[{"xs_", ",", "deltaX_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"varXS", "=", 
       RowBox[{"Sort", "[", "xs", "]"}]}], ",", "\[IndentingNewLine]", "n", 
      ",", "intervalLen", ",", "intervals", ",", "values", ",", "labels"}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"n", "=", 
      RowBox[{
       RowBox[{"Round", "[", 
        RowBox[{"Last", "[", "varXS", "]"}], "]"}], "+", "1"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"intervalLen", "=", 
      RowBox[{"N", "@", 
       FractionBox["n", "deltaX"]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"intervals", "=", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{"i", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", "n", ",", "intervalLen"}], "}"}]}], 
         "]"}], ",", "2", ",", "1"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"values", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Count", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"intervals", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "1"}], "\[RightDoubleBracket]"}], "<=", "#", 
             "\[LessEqual]", 
             RowBox[{"intervals", "\[LeftDoubleBracket]", 
              RowBox[{"i", ",", "2"}], "\[RightDoubleBracket]"}]}], "&"}], "/@",
            "varXS"}], ",", "True"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "deltaX"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"labels", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"StringJoin", "[", 
         RowBox[{"\"\<[\>\"", ",", 
          RowBox[{"ToString", "[", 
           RowBox[{
           "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "]"}],
           ",", "\"\<,\>\"", ",", 
          RowBox[{"ToString", "[", 
           RowBox[{
           "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], "]"}],
           ",", "\"\<)\>\""}], "]"}], "&"}], "/@", "intervals"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"values", ",", 
       RowBox[{"BarChart", "[", 
        RowBox[{"values", ",", 
         RowBox[{"ChartLabels", "->", "labels"}], ",", 
         RowBox[{"ImageSize", "\[Rule]", "Medium"}]}], "]"}]}], "}"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]}], "Input",
